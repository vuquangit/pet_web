{
  "name": "reactjs-19",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack serve --config config/webpack.dev.js --port=3000",
    "build:dev": "rm -rf build/ && webpack --config config/webpack.dev.js",
    "build:stg": "rm -rf build/ && webpack --config config/webpack.stg.js",
    "build:prod": "rm -rf build/ && webpack --config config/webpack.prod.js",
    "server": "node server/index.js",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "eslint --ext ts --ext tsx --ext js --ext jsx",
    "format": "prettier --write **/*.{js,ts,tsx}",
    "test:unit": "jest -u --coverage",
    "test:unit:ci": "jest --silent --maxWorkers=2 --collectCoverage=false",
    "test:e2e:open": "cypress open",
    "test:e2e:run": "cypress run",
    "test:e2e:ci": "start-server-and-test 'yarn dev' http://localhost:3000 'yarn test:e2e:run'",
    "prepare": "husky",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
